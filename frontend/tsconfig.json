{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "jsx": "react",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "module": "esnext"
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
